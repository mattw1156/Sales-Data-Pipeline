BucketIndex = 
VAR v = CustomerRevenue[TotalRevenue]
RETURN
SWITCH (
    TRUE(),
    ISBLANK(v), 98,          -- push Unknown near end
    v < 0, 0,
    v <= 5000, 1,
    v <= 10000, 2,
    v <= 15000, 3,
    v <= 20000, 4,
    v <= 25000, 5,
    v <= 30000, 6,
    v <= 35000, 7,
    v <= 40000, 8,
    v <= 45000, 9,
    v <= 50000, 10,
    11                        -- 50k
)